<script src="/js/jquery2.2.4.min.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script>
    $('#logout').on('click',function(){
        $.ajax({
            url:'/logout',
            type:'post',
            success:function(data){
                if(data=='1'){
                    window.location.href='/';
                }
            }
        })
    });
</script>
</html>